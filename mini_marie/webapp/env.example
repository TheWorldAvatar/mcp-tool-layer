# MOP Extraction Agent - Web Extension Environment Variables
# Copy this file to .env and fill in your actual values
# Usage: cp env.example .env

# Required: OpenAI API Key for LLM access
OPENAI_API_KEY=your-openai-api-key-here

# Optional: Flask secret key for session encryption
# If not set, a random key will be generated (not suitable for production)
FLASK_SECRET_KEY=your-secret-key-for-production

# Optional: Flask environment (development or production)
FLASK_ENV=development

# Optional: Enable debug mode (true or false)
FLASK_DEBUG=true

